<% @client_logs.each do |client_log|%>
  <% if client_log.nurse_log %>
    <div class='card mb-3' >
      <div class='card-body'>
        <div>
          <%= client_log.nurse_log %>
        </div>
        <div>
          <%= format_date(client_log.updated_at) %>
        </div>
        <div>
          Created by <%= client_log.user.full_name %>
        </div>
        <div class= 'card-link'>
          <%= link_to "edit", edit_client_client_log_path(@client, client_log, location: @location_param, log_type: @client_log_param) %>
        </div>
      </div>
    </div>
  <% end %>
<% end %>