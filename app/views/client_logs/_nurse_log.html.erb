<% if @log_present %>
  <div>
    Attention: Doctor Logs have already been created for this client on this date. Please review 
    <%= link_to 'Doctor Logs Here.', client_client_logs_path(@client, location: @location_param, log_type: 'doctor log', log_date: @log_date_param )%>
  </div>
<% end %>
<% @client_logs.each do |client_log|%>
  <div class='card mb-3' >
    <div class='card-body'>
      <div>
        <%= client_log.nurse_log %>
      </div>
      <div>
        <%= format_date(client_log.updated_at) %>
      </div>
      <div>
        Created by <%= client_log.user.full_name %>
      </div>
      <div class= 'card-link'>
        <%= link_to "edit", edit_client_client_log_path(@client, client_log, location: @location_param, log_type: @client_log_param, log_date: @log_date_param) %>
      </div>
    </div>
  </div>
<% end %>